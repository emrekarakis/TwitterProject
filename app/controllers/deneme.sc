val map1: Map[String, Seq[String]] = Map("abcde"-> Seq("https://google.com","https://twitter.com","https://facebook.com"))
val map2: Map[String, Seq[String]] = Map("abcde"-> Seq("https://google.com","https://bbc.com","https://galatasaray.org"))
def combineMaps( map1: Map[String,Seq[String]], map2: Map[String,Seq[String]]):  Map[String,Seq[String]] = {
  val resVal: Map[String, Seq[String]] = (map1.keySet ++ map2.keySet).map{ e:String  =>
    e -> (map1.getOrElse(e,Seq.empty) ++ map2.getOrElse(e,Seq.empty))
  }.toMap
  // (map1.keySet ++ map2.keySet).map (i=> (i,map1.getOrElse(i,0) + map2.getOrElse(i,0))).toMap
  resVal
}


